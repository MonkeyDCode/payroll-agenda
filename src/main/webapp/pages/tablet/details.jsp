              <div class="col s6">
                <div class="data-write rci-date">
                  <div class="input-field">
                    <input id="inpNum" type="text" name="" maxlength="10" minlength="10" value="Oct 13, 2017">
                    <label for="inpNum">Fecha</label>
                    <p class="form-tips">Introduce tu ...</p>
                  </div>
                </div>
              </div>
              <div class="col s6">
                <div class="data-write rci-hour">
                  <div class="input-field">
                    <input id="inpNum" type="text" name="" maxlength="10" minlength="10" value="15:35">
                    <label for="inpNum">Hora</label>
                    <p class="form-tips">Introduce tu ...</p>
                  </div>
                </div>
              </div>
              <div class="col s12 cont-btn">
                <a href="#" class="btn little red">Guardar</a>
              </div>
